#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'xp-pen-userland-config-util==0.1.1','gui_scripts','xp_pen_userland_config_util'
__requires__ = 'xp-pen-userland-config-util==0.1.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('xp-pen-userland-config-util==0.1.1', 'gui_scripts', 'xp_pen_userland_config_util')()
    )
